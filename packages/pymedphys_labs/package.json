{
  "dependencies": {
    "pymedphys_utilities": "~0.8.0-dev.16"
  },
  "name": "pymedphys_labs",
  "private": true,
  "scripts": {
    "pip:install:dev": "pip install -e .",
    "pip:install:prod": "pip install .",
    "pip:install:test": "pip install .[test]",
    "postversion": "yarn propegate:version",
    "propegate:version": "python -m pymedphys_monomanage.propegate.versions",
    "pypi:build": "python setup.py sdist bdist_wheel",
    "pypi:clean": "python -c \"import shutil; shutil.rmtree('dist'); shutil.rmtree('build')\"",
    "pypi:upload": "twine upload dist/*"
  },
  "version": "0.8.0-dev.16"
}