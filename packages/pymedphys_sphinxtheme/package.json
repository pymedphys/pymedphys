{
  "name": "pymedphys_sphinxtheme",
  "main": "js/theme.js",
  "version": "0.2.7",
  "private": true,
  "scripts": {
    "bootstrap": "yarn --production && yarn webpack:prod",
    "pip:install:dev": "yarn bootstrap && pip install -e .",
    "pip:install:prod": "yarn bootstrap && pip install .",
    "pip:install:test": "yarn bootstrap && pip install .[test]",
    "postversion": "yarn propegate:version",
    "propegate:version": "python ../../scripts/propegate_versions.py",
    "pypi:build": "python setup.py sdist bdist_wheel",
    "pypi:upload": "twine upload dist/*",
    "webpack:dev": "yarn webpack-dev-server --open --config webpack.dev.js",
    "webpack:prod": "yarn webpack --config webpack.prod.js"
  },
  "dependencies": {
    "bourbon": "~4.3",
    "bourbon-neat": "~1.9",
    "browserify": "^13.0.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "imports-loader": "^0.8.0",
    "jquery": "^3.4.1",
    "lato-font": "^3.0.0",
    "matchdep": "~1.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "roboto-fontface": "^0.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "wyrm": "^1.0.9"
  },
  "devDependencies": {
    "webpack-dev-server": "^3.3.1"
  }
}
