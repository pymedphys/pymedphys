{
  "name": "pymedphys",
  "private": true,
  "scripts": {
    "bootstrap": "yarn bootstrap:dev",
    "bootstrap:dev": "yarn && yarn nbstripout:install && yarn install:dev",
    "bootstrap:dev:all": "yarn bootstrap:dev && pip install -e .[docs,testing,linting]",
    "docker:internal": "yarn install:prod",
    "git:clean": "git clean -fxd",
    "git:undo": "git reset --soft HEAD~1",
    "install:dev": "yarn lerna run pip:install:dev --concurrency 1 && pip install -e .",
    "install:prod": "yarn lerna run pip:install:prod --concurrency 1 && pip install .",
    "lerna:pypi:build": "lerna run pypi:build",
    "lerna:pypi:upload": "lerna run pypi:upload",
    "nbstripout:install": "pip install nbstripout && nbstripout --install",
    "netlify": "yarn netlify:install && yarn netlify:build",
    "netlify:build": "sphinx-build -W docs docs/_build/html",
    "netlify:install": "yarn install:prod && pip install .[docs]",
    "pypi:build": "python setup.py sdist bdist_wheel",
    "pypi:upload": "twine upload dist/*",
    "pytest:pylint": "pytest --pylint --pylint-error-types=EF --pylint-jobs=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "nbstripout --is-installed"
    }
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lerna": "^3.13.3"
  },
  "dependencies": {
    "bourbon": "^5.1.0",
    "neat": "^1.2.2"
  }
}